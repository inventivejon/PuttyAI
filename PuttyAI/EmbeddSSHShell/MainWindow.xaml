<Window x:Class="EmbeddSSHShell.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EmbeddSSHShell"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="BtnRun" Content="Run" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Click="BtnRun_Click"></Button>
        <Button x:Name="BtnSet" Content="Set Parent" HorizontalAlignment="Left" Margin="90,10,0,0" VerticalAlignment="Top" Click="BtnSet_Click"></Button>
        <TabControl TabStripPlacement="Top" SelectionChanged="TC_SetFocus" Margin="0,34,10.2,-0.2">
            <TabItem x:Name="winFormsHost" Header="TabItem with Buttons" />
            <TabItem Name="tab2" Header="Content" />
            <TabItem Name="tab3" Header="Help" />
        </TabControl>
    </Grid>
</Window>
